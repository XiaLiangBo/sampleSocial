<script id="tmpl-GithubReleases" type="text/html">
  <div class="container githubReleases" data-name="{{reponame}}" data-login="{{login}}">
        <table class="table table-striped">
            <thead>
                <tr class="header">
                    <th colspan="6">
                        Releases for {{name}}
                    </th>
                </tr>
            </thead>
            <tr class="table-title">
                <td>name</td>
                <td>tag_name</td>
                <td>created_at</td>
                <td>published_at</td>
                <td>target_commitish</td>
                <td>author</td>
            </tr>
            {{#each releases}}
            <tr>
                <td class="message" data-release-id="{{id}}" data-release-name="{{name}}">
                    <div>
                    <span class="messageTitle">{{name}}</span>
                    </div>
                </td>
                <td class="sha">
                    <div>
                    <span class="created-at">{{tag_name}}</span>
                    <i class="glyphicon glyphicon-edit edit"></i>
                    </div>
                </td>
                <td class="sha">
                    <div><span class="created-at">{{created_at}}</span></div>
                </td>
                <td class="sha">
                    <div><span class="published-at">{{published_at}}</span></div>
                </td>
                <td class="sha">
                    <div><span class="target-commitish">{{target_commitish}}</span></div>
                </td>
                <td class="sha">
                    <div><span class="messageTitle">{{author.login}} released</span></div>
                </td>
            </tr>
            {{/each}}
        </table>
	</div>
</script>