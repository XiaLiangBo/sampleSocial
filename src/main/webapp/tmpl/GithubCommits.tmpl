<script id="tmpl-GithubCommits" type="text/html">
  <div class="container githubCommits">
		<table class="table">
			<thead>
				<tr class="header">
					<th colspan="2">
						Commits History
					</th>
				</tr>
			</thead>
			<tbody>
				{{#each commits}}
				<tr>
					<td class="message">
						<div>{{commit.message}}</div>
						<div class="author">{{commit.committer.name}}&nbsp;authored&nbsp;{{date commit.committer.date}}</div>
					</td>
					<td class="sha">
						<span>{{sha}}</span>
					</td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</script>